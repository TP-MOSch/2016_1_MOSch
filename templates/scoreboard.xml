<fest:template xmlns:fest="http://fest.mail.ru" context_name="data">
			<div class="content__wrapper">
				<div class="scoreboard">
					<div class="scoreboard__title">Scoreboard</div>
					<div class="scoreboard__table">
						<fest:for iterate="data.items" index="i" value="v">
							<div class="record">
								<div class="record__rate"><fest:value>v.rate</fest:value></div>
								<div class="record__username"><fest:value>v.username</fest:value></div>
							</div>
						</fest:for>
					</div>
				</div>
			</div>
</fest:template>
